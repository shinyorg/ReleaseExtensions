{
    "name": "Shiny.Push",
    "description": "Native OS Push Providers",
    "samples": [
        {
            "description": "Sample",
            "urlPath": "Push-Native"
        }
    ],
    "services": [
        {
            "name": "Shiny.Push.IPushManager",
            "startup": "UsePush",
            "bgDelegate": "Shiny.Push.IPushDelegate",
            "bgDelegateRequired": true,
            "static": "ShinyPush",
            "ios": {
                "usesPush": true
            },
            "android": {
            },
            "uwp": {
                "backgroundTasks": [ "pushNotification" ]
            }
        }
    ]
}